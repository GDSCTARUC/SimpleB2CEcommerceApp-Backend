{
  "Routes": [
    {
      "DownstreamPathTemplate": "/cartServer/{catchAll}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5010
        }
      ],
      "UpstreamPathTemplate": "/api/cart/{catchAll}",
      "UpstreamHttpMethod": [
        "Options",
        "Get",
        "Post",
        "Patch",
        "Delete"
      ],
      "Priority": 1
    },
    {
      "DownstreamPathTemplate": "/productServer/{catchAll}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5020
        }
      ],
      "UpstreamPathTemplate": "/api/product/{catchAll}",
      "UpstreamHttpMethod": [
        "Options",
        "Get",
        "Post",
        "Patch",
        "Delete"
      ],
      "Priority": 2
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5000"
  }
}